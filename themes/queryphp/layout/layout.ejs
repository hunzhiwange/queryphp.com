        <link rel="alternate" hreflang="x-default" href="https://queryphp.com/<%- page.path %>">
        <link rel="alternate" hreflang="zh" href="https://queryphp.com/<%- page.path %>">

        <link rel="apple-touch-icon" sizes="57x57" href="<%- url_for("/images/icons/apple-icon-57x57.png") %>">
